        <Image x:Name="p4"  Height="100" Margin="497,72,195,0" VerticalAlignment="Top" Width="100" Source="Resources/Images/p4.bmp" Stretch="Fill"/>
        <Image x:Name="p5" HorizontalAlignment="Left" Height="100" Margin="632,72,0,248" Width="100" Source="Resources/Images/p5.bmp" Stretch="Fill"/>
    </Grid>
</Window>
